{
	"name": "uni-app",
	"version": "1.0.0",
	"scripts": {
		"svgo": "svgo -f /static/images",
		"lint:prettier": "prettier --write .",
		"prepare": "husky",
		"lint:lint-staged": "lint-staged",
		"commit": "git pull && git add -A && git-cz && git push"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"commitizen": "^4.3.0",
		"cz-git": "^1.9.1",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"svgo": "^3.2.0",
		"unplugin-auto-import": "^0.17.5"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
	"uni-app": {
		"scripts": {
			"mp-dingtalk": {
				"title": "钉钉小程序",
				"env": {
					"UNI_PLATFORM": "mp-alipay"
				},
				"define": {
					"MP-DINGTALK": true
				}
			},
			"h5-dev": {
				"title": "H5 开发环境",
				"browser": "chrome",
				"env": {
					"UNI_PLATFORM": "h5",
					"ENV_TYPE": "development"
				},
				"define": {
					"CUSTOM-CONST": true
				}
			},
			"h5-test": {
				"title": "H5 测试环境",
				"browser": "chrome",
				"env": {
					"UNI_PLATFORM": "h5",
					"ENV_TYPE": "test"
				},
				"define": {
					"CUSTOM-CONST": true
				}
			},
			"h5-pro": {
				"title": "H5 生产环境",
				"browser": "chrome",
				"env": {
					"UNI_PLATFORM": "h5",
					"ENV_TYPE": "production"
				},
				"define": {
					"CUSTOM-CONST": true
				}
			}
		}
	}
}
